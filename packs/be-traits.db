{"_id": "60d46ecb-72f1-411b-b42d-dd8713c8f9a4", "name": "Codebreaker", "permission": {"default": 0}, "type": "trait", "img": "icons/sundries/gaming/dice-runed-brown.webp", "flags": {}, "data": {"traittype": "die", "pointCost": 3, "text": "This Psychologist has a 1D infamous reputation among his colleagues and anyone else who knows him. At one point in the past, he used his psychological powers to force an innocent mind open and take what he wanted. He is a codebreaker.\n\nRestrictions: Psychologist-type characters only.", "hasAptitudeModifier": false, "aptitudeModifier": 0, "aptitudeTarget": "", "hasDieModifier": false, "dieModifier": 0, "dieModifierTarget": "", "addsReputation": false, "reputationName": "", "reputationDice": 0, "reputationInfamous": false, "addsAffiliation": false, "affiliationName": "", "affiliationDice": 0}}
