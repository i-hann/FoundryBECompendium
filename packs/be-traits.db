{"_id": "592055af-9614-440f-ab71-1c88960446b7", "name": "Wise Aphorisms", "permission": {"default": 0}, "type": "trait", "img": "icons/magic/lightning/bolt-blue.webp", "flags": {}, "data": {"traittype": "call-on", "pointCost": 4, "text": "Old men are full of pertinent yet trite sayings. Use this a call-on for Persuasion, Oratory, Interrogation or Seduction if the player can come up with some appropriately sagacious dialogue to throw into play at the right moment.", "hasAptitudeModifier": false, "aptitudeModifier": 0, "aptitudeTarget": "", "hasDieModifier": false, "dieModifier": 0, "dieModifierTarget": "", "addsReputation": false, "reputationName": "", "reputationDice": 0, "reputationInfamous": false, "addsAffiliation": false, "affiliationName": "", "affiliationDice": 0}}
